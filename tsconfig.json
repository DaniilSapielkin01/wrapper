{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src"
  },

  "paths": {
    "@/*": ["./src/*"]
  },

  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/assets/styles/theme-mui.ts",
    "src/contracts/provider/index.ts—á",
    "src/contracts/provider/wethContract.ts"
  ],
  "exclude": ["node_modules"]
}
